{
    "manifest_version": 2,
    "name": "dereferrer",
    "description": "Referer spoofing",
    "version": "0.0.2",
    "icons": {
	"128": "icons/128.png"
    },

    "page_action": {
	"default_icon": {
	    "19": "icons/19.png"
	}
    },
    "background": {
	"scripts": ["background.js"]
    },
    "options_page": "options.html",
    "permissions": [
	"tabs",
	"storage",
	"webRequest",
	"webRequestBlocking",
	"<all_urls>"
    ]
}
